169. Majority Element
class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int curr = nums[0];
        int cnt =0;
        for(int i=0;i<nums.size();i++){
            if(curr == nums[i])
                cnt ++;   
            else
                cnt --;
            if(cnt == 0)
                curr = nums[i+1];
        }
        return curr;
        
    }
};
