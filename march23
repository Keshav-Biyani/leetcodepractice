#include <iostream>
#include <vector>
using namespace std;
void subset(vector<int> arr, int index, vector<int> output, vector<vector<int>> &ans)
{
    if (index >= arr.size())
    {
        ans.push_back(output);
        return;
    }
    subset(arr, index + 1, output, ans);
    int element = arr[index];
    output.push_back(element);
    subset(arr, index + 1, output, ans);
}

int main()
{
    vector<vector<int>> ans;
    vector<int> output;
    vector<int> arr = {1, 2, 3};
    subset(arr, 0, output, ans);
    for (int i = 0; i < ans.size(); i++)
    {
        for (int j = 0; j < ans[i].size(); j++)
        {
            cout << ans[i][j] << " ";
        }
        cout << endl;
    }

    return 0;
}
///Subsequences of String
#include <bits/stdc++.h> 
void subset(string str,int index , string output,vector<string>& ans)
{
	if(index>=str.length()){
		if(output.length()>0)
		ans.push_back(output);
		return ;
	}
	subset(str,index+1,output,ans);
	char element = str[index];
	output.push_back(element);
	subset(str,index+1,output,ans);
}
vector<string> subsequences(string str)
{
	vector<string> ans ;
	string output = "";
	subset(str,0,output,ans);	
	// Write your code here
	return ans;
	
}
